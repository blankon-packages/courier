@HAVE_SGML_TRUE@$(GITFILES): %: %.git
@HAVE_SGML_TRUE@	perl -p -e 's/\$$Id:[^\$$]*/\$$Id: '"`git log --pretty=format:\"%H\" -n 1 -- $<`-`date +%Y%m%d%H%M%S`/" <$< >$@.tmp
@HAVE_SGML_TRUE@	mv -f $@.tmp $@

@HAVE_SGML_TRUE@imapd.html.in: imapd.sgml ../docbook/sgml2html
@HAVE_SGML_TRUE@	../docbook/sgml2html imapd.sgml imapd.html.in

@HAVE_SGML_TRUE@imapd.8.in: imapd.sgml ../docbook/sgml2man
@HAVE_SGML_TRUE@	../docbook/sgml2man imapd.sgml imapd.8.in "--stringparam man.base.url.for.relative.links http://www.courier-mta.org/"
@HAVE_SGML_TRUE@	mv imapd.8 imapd.8.in

